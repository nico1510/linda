<pretty-config xmlns="http://ocpsoft.org/prettyfaces/3.3.3" 
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
               xsi:schemaLocation="http://ocpsoft.org/prettyfaces/3.3.3
               http://ocpsoft.org/xml/ns/prettyfaces/ocpsoft-pretty-faces-3.3.3.xsd">
            
	<url-mapping id="datasets">
		<pattern value="/datasets" />
		<view-id value="/faces/datasets.xhtml" />
	</url-mapping>
        
	<url-mapping id="upload">
		<pattern value="/upload" />
		<view-id value="/faces/upload.xhtml" />
	</url-mapping>

	<url-mapping id="home">
		<pattern value="/home" />
		<view-id value="/faces/index.xhtml" />
	</url-mapping>
        
	<url-mapping id="blog">
		<pattern value="/blog" />
		<view-id value="/faces/feeds.xhtml" />
	</url-mapping>

	<url-mapping id="about">
		<pattern value="/about" />
		<view-id value="/faces/about.xhtml" />
	</url-mapping>
        
	<url-mapping id="admin">
		<pattern value="/admin" />
		<view-id value="/faces/admin.xhtml" />
	</url-mapping>
        
        <url-mapping id="details">
	        <pattern value="/datasets/#{id}" />
		<view-id value="/faces/details.xhtml" />
	</url-mapping>
     
	<url-mapping id="download">
		<pattern value="/datasets/#{nodeid}/#{fileid}" />
		<view-id value="/faces/download.xhtml" />
	</url-mapping>
        
	<url-mapping id="display">
		<pattern value="/datasets/#{nodeid}/#{toolid}/#{uri}" />
		<view-id value="/faces/display.xhtml" />
	</url-mapping>
        
	<url-mapping id="triples">
		<pattern value="/#{nodeid}" />
		<view-id value="/faces/triples.xhtml" />
	</url-mapping>
</pretty-config>